{
    "name": "Phoenix-MD",
    "version": "1.0.0",
    "description": "Phoenix-MD Simple WhatsApp Bot",
    "logo": "https://i.imgur.com/T78fhJk.jpeg",
    "keywords": ["whatsapp", "bot", "md", "multi-device", "Phoenix", "Phoenix-MD"],
    "repository": "https://github.com/AbhishekSuresh2/Phoenix-MD",
    "stack": "container",
    "env": {
        "SESSION_ID": {
            "description": "Enter Your Session ID",
            "required": true
        },
        "HANDLERS": {
            "description": "Enter Prefix For Commands",
            "required": true,
            "value": ","
        },
        "SUDO": {
            "description": "Enter Numbers With Country Code Without + Separated With , To Add More Numbers",
            "required": false
            "value": "919074692450"
        },
        "OWNER_NAME": {
            "description": "Enter Your Owner Name",
            "required": false
            "value": "Abhishek Suresh"
        },
        "OWNER_NUMBER": {
            "description": "Enter Your Owner Number",
            "required": false
            "value": "919074692450"
        },
        "BOT_NAME": {
            "description": "Enter Your Bot Name",
            "required": false
            "value": "Phoenix-MD"
        },
        "WORK_TYPE": {
            "description": "Enter Your Mode Type public For Public Otherwise Type private",
            "required": false
            "value": "public"
        },
        "HEROKU_APP_NAME": {
            "description": "Enter Your Heroku App Name Same As Above You Entered",
            "required": true
        },
        "HEROKU_API_KEY": {
            "description": "Enter Your Heroku Account Api Key. Get It From https://dashboard.heroku.com/account",
            "required": true
        },
        "AUTO_STATUS_READ": {
            "description": "If You Want To View Status Automatically, Type true. Otherwise, Type false",
            "required": false,
            "value": "true"
        }
    },
    "buildpacks": [
        {
            "url": "heroku/nodejs"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
        }
    ]
}
